{
  "name": "js-linq",
  "version": "2.0.0",
  "description": "A Javascript version of .NET's LINQ to Objects, with some query operations inspired by MoreLinq.",
  "author": "Kurtis Jones <battousai999@gmail.com>",
  "homepage": "https://github.com/battousai999/js-linq",
  "license": "MIT",
  "main": "dist-node/jslinq.js",
  "module": "jslinq.js",
  "jspm": {
    "main": "dist/jslinq.js"
  },
  "config": {
    "banner": "/* $linq Version $npm_package_version (by Kurtis Jones @ $npm_package_homepage) */"
  },
  "scripts": {
    "build-node": "npx babel --presets es2015 --plugins=transform-es2015-modules-commonjs,transform-runtime jslinq.js --out-file dist-node/jslinq.js",
    "build-browser": "npm run browserify && npm run uglify-browser",
    "browserify": "npx browserify --standalone Linq dist-node/jslinq.js > dist/jslinq.js",
    "uglify-browser": "cross-var npx uglifyjs -mc -b \"beautify=false,preamble='$npm_package_config_banner'\" -- dist/jslinq.js > dist/jslinq.min.js",
    "uglify": "cross-var npx uglify-es -mc -b \"beautify=false,preamble='$npm_package_config_banner'\" -- jslinq.js > jslinq.min.js",
    "clean": "rimraf dist dist-node && mkdirp dist dist-node",
    "build": "npm run clean && npm run build-node && npm run build-browser && npm run uglify",
    "prepublish": "npm run build",
    "test": "karma start",
    "test-babel-full": "karma start karma-babel-full.conf.js",
    "test-full": "karma start karma-full.conf.js",
    "uglify-grunt": "grunt uglify-both",
    "nuget-clean": "rimraf nuget-pub/content && mkdirp nuget-pub/content/Scripts",
    "nuget-copy-es6": "npx cpy jslinq.js jslinq.min.js nuget-pub/content/Scripts",
    "nuget-copy-amd": "npx cpy dist/jslinq.js nuget-pub/content/Scripts --rename=jslinq.amd.js && npx cpy dist/jslinq.min.js nuget-pub/content/Scripts --rename=jslinq.amd.min.js",
    "nuget-build": "npm run build && npm run nuget-clean && npm run nuget-copy-es6 && npm run nuget-copy-amd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/battousai999/js-linq.git"
  },
  "bugs": {
    "url": "https://github.com/battousai999/js-linq/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.35",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^16.2.3",
    "cpy-cli": "^2.0.0",
    "cross-var": "^1.1.0",
    "derequire": "^2.0.6",
    "eslint": "^4.19.1",
    "grunt": "^1.0.3",
    "grunt-babel": "^7.0.0",
    "grunt-contrib-uglify-es": "^3.3.0",
    "grunt-karma": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.5",
    "load-grunt-tasks": "^3.4.1",
    "mkdirp": "^0.5.1",
    "phantomjs-prebuilt": "^2.1.16",
    "rimraf": "^2.6.2",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.4.9",
    "webpack": "^3.10.0"
  }
}
